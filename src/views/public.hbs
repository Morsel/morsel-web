<!DOCTYPE html>
<html id="ng-app" ng-app="Morsel.public" ng-controller="AppCtrl">
  <head>
    {{> partials/headContent}}
    <!-- compiled CSS -->
    <% styles.forEach( function ( file ) { %>
    <link rel="stylesheet" type="text/css" href="<%= file %>" />
    <% }); %>
    <!-- compiled JavaScript -->
    <% scripts.forEach( function ( file ) { %>
    <script type="text/javascript" src="<%= file %>"></script>
    <% }); %>
    {{> partials/mixpanel}}
  </head>
  <body>
    {{> partials/pageWrapper}}
    {{> partials/ga}}
  </body>
</html>
