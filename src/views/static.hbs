<!DOCTYPE html>
<html id="ng-app" ng-app="Morsel.static" ng-controller="StaticCtrl">
  <head>
    {{> headContent}}
    <!-- compiled CSS -->
    <% styles.forEach( function ( file ) { %>
    <link rel="stylesheet" type="text/css" href="<%= file %>" />
    <% }); %>
    <!-- compiled JavaScript -->
    <% scripts.forEach( function ( file ) { %>
    <script type="text/javascript" src="<%= file %>"></script>
    <% }); %>
  </head>
  <body class="static">
    {{> pageWrapper}}
    {{> ga}}
  </body>
</html>